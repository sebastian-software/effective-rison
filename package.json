  "description": "URI friendly encoding for JSON structures",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nanonid/rison.git"
  },
  "main": "src/rison.js",
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@9.12.3",
  "devDependencies": {
    "prettier": "^3.3.3",
    "tsup": "^8.5.0",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4"
  }
}
