{
  "name": "rison",
  "version": "1.0.0",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc -p .",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "pnpm run build"
  },
  "author": "Metaweb Technologies <devolopers@freebase.com>",
  "description": "URI friendly encoding for JSON structures",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastian-software/rison.git"
  },
  "type": "module",
  "main": "dist/rison.js",
  "exports": "./dist/rison.js",
  "types": "dist/rison.d.ts",
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.0.0",
  "dependencies": {
    "lz-string": "^1.5.0"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "homepage": "https://github.com/sebastian-software/rison#readme",
  "bugs": {
    "url": "https://github.com/sebastian-software/rison/issues"
  },
  "files": [
    "dist/"
  ],
  "sideEffects": false,
  "keywords": [
    "rison",
    "uri",
    "encoding",
    "serialization",
    "json",
    "typescript",
    "esm",
    "url",
    "compression"
