{
  "name": "rison",
  "version": "0.1.3",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc -p .",
    "format": "prettier -w .",
    "build": "tsup src/rison.js --format cjs,esm --out-dir dist --sourcemap --clean",
    "prepublishOnly": "pnpm run build"
  },
